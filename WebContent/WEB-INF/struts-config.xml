<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<!--
     Squelette du fichier de configuration du controleur STRUTS
-->


<struts-config>


<!-- ================================================ Form Bean Definitions -->

    <form-beans>
			<form-bean name="BeanFilm" type="struts.actionForm.BeanFilm"></form-bean>
			<form-bean name="BeanPersonne" type="struts.actionForm.BeanPersonne"></form-bean>
			<form-bean name="BeanProfessionnel" type="struts.actionForm.BeanProfessionnel"></form-bean>
			<form-bean name="BeanRecompense" type="struts.actionForm.BeanRecompense"></form-bean>
			<form-bean name="BeanRecompenseFilm" type="struts.actionForm.BeanRecompenseFilm"></form-bean>
			<form-bean name="BeanRecompensePersonne" type="struts.actionForm.BeanRecompensePersonne"></form-bean>
			<form-bean name="BeanTypeRecompenseFilm" type="struts.actionForm.BeanTypeRecompenseFilm"></form-bean>
			<form-bean name="BeanTypeRecompensePersonne" type="struts.actionForm.BeanTypeRecompensePersonne"></form-bean>
			<form-bean name="BeanVote" type="struts.actionForm.BeanVote"></form-bean>


    </form-beans>


<!-- ========================================= Global Exception Definitions -->

    <global-exceptions>

    </global-exceptions>


<!-- =========================================== Global Forward Definitions -->

    <global-forwards>

    </global-forwards>


<!-- =========================================== Action Mapping Definitions -->

    <action-mappings>
    	<!--action path="" forward="">
    		
    	</action-->
    

    </action-mappings>


<!-- ======================================== Message Resources Definitions -->

    <message-resources parameter="MessageResources" />

</struts-config>

